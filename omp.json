{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "shell_integration": true,
  "terminal_background": "p:base",
  "palette": {
    "rosewater": "#f5e0dc",
    "flamingo": "#f2cdcd",
    "pink": "#f5c2e7",
    "mauve": "#cba6f7",
    "red": "#f38ba8",
    "maroon": "#eba0ac",
    "peach": "#fab387",
    "yellow": "#f9e2af",
    "green": "#a6e3a1",
    "teal": "#94e2d5",
    "sky": "#89dceb",
    "sapphier": "#74c7ec",
    "blue": "#89b4fa",
    "lavender": "#b4befe",
    "text": "#cdd6f4",
    "subtext1": "#bac2de",
    "subtext0": "#a6adc8",
    "overlay2": "#9399b2",
    "overlay1": "#7f849c",
    "overlay0": "#6c7086",
    "surface2": "#585b70",
    "surface1": "#45475a",
    "surface0": "#313244",
    "base": "#1e1e2e",
    "mantle": "#181825",
    "crust": "#11111b"
  },
  "enable_cursor_positioning": true,
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "p:text",
    "template": "\uf178 "
  },
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "upgrade": {
    "source": "cdn",
    "interval": "168h",
    "auto": false,
    "notice": false
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "p:blue",
          "foreground": "p:surface0",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "{{.Icon}} ",
          "type": "os"
        },
        {
          "powerline_symbol": "\ue0b0",
          "template": "{{ if false }}{{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }}{{ if .SSHSession }}@{{.HostName}}{{ end }} {{ end }}",
          "foreground": "p:surface0",
          "background": "p:blue",
          "type": "session",
          "style": "powerline"
        },
        {
          "properties": {
            "style": "full"
          },
          "template": " {{ path .Path .Location }} ",
          "foreground": "p:surface0",
          "powerline_symbol": "\ue0b0",
          "background": "p:pink",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_max_length": 25,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ if .UpstreamURL }}{{ url .UpstreamIcon .UpstreamURL }} {{ end }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "foreground": "p:surface0",
          "powerline_symbol": "\ue0b0",
          "background": "p:lavender",
          "type": "git",
          "style": "powerline"
        },
        {
          "template": " \uf0e7 ",
          "foreground": "p:surface0",
          "powerline_symbol": "\ue0b0",
          "background": "p:yellow",
          "type": "root",
          "style": "powerline"
        },
        {
          "properties": {
            "always_enabled": true
          },
          "trailing_diamond": "\ue0b4",
          "template": " {{ if gt .Code 0 }}\uf00d{{ else }}\uf00c{{ end }} ",
          "foreground": "p:surface0",
          "background": "p:green",
          "type": "status",
          "style": "diamond",
          "background_templates": [
            "{{ if gt .Code 0 }}p:red{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "template": " {{ .Name }} ",
          "foreground": "p:surface0",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "background": "p:pink",
          "type": "shell",
          "style": "diamond",
          "properties": {
            "mapped_shell_names": {
              "pwsh": "PS"
            }
          }
        },
        {
          "background": "p:blue",
          "foreground": "p:surface0",
          "trailing_diamond": "\ue0b4",
          "style": "diamond",
          "template": " at {{ .CurrentDate | date \"15:04:05\" }}",
          "type": "time"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "background": "transparent",
          "foreground": "p:text",
          "style": "plain",
          "template": "\udb80\udd42"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
